# LibreOffice 配置
LIBREOFFICE_HOST=libreoffice
LIBREOFFICE_CONTAINER_NAME=libreoffice
LIBREOFFICE_PORT=3000
UNO_PORT=8100

# 临时文件配置
TEMP_DIR=/temp
UPLOADS_SUBDIR=uploads
CONVERTED_SUBDIR=converted
CLEANUP_LOG_FILE=.cleanup.log

# 清理策略配置
MAX_TEMP_SIZE_MB=500
MAX_FILE_AGE_HOURS=1
CLEANUP_INTERVAL_HOURS=1
ENABLE_STARTUP_CLEANUP=true
ENABLE_ON_REQUEST_CLEANUP=true

# API 配置
API_PREFIX=/api/v1
MAX_UPLOAD_SIZE_MB=50
ALLOWED_ORIGINS=["*"]

# 转换配置
CONVERSION_TIMEOUT_SECONDS=120
MAX_RETRY_ATTEMPTS=3
